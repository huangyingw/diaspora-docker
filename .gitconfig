[remote "dhovart"]
    url = git@github.com:dhovart/diaspora-docker.git
    fetch = +refs/heads/*:refs/remotes/dhovart/*
[remote "github"]
    url = git@github.com:huangyingw/diaspora-docker.git
    fetch = +refs/heads/*:refs/remotes/github/*
[push]
    remote = github
[gsync]
    remote = dhovart
    branch = master
[merge]
    allow = true
[branch "dev"]
    remote = github
    merge = refs/heads/dev
[branch "master"]
    remote = dhovart
    merge = refs/heads/master
[commit]
    message = n
[vrun]
    silent = false
    async = false
